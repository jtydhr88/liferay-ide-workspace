<?xml version="1.0" encoding="UTF-8"?>
<feature
	id="com.liferay.ide.workspace"
	label="%featureName"
	plugin="com.liferay.ide.workspace.ui"
	provider-name="%providerName"
	version="${version}"
>

	<description url="http://www.liferay.com/community/liferay-projects/liferay-ide/overview">
	  %description
	</description>

	<copyright url="http://www.liferay.com/products/liferay-portal/lgpl">
	  %copyright
	</copyright>

	<license url="%licenseUrl">
	  %license
	</license>

	<requires>
		<import feature="org.eclipse.sapphire" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.java" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.java.jdt" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.modeling.xml" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.osgi" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.platform" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.ui" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.ui.swt.gef" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import feature="org.eclipse.sapphire.ui.swt.xml.editor" match="greaterOrEqual" version="9.1.1.201712191343" />
		<import plugin="org.eclipse.jst.j2ee.core" />
		<import plugin="org.eclipse.jst.j2ee" />
		<import plugin="org.eclipse.jst.jsp.core" />
		<import plugin="org.eclipse.jst.jsp.ui" />
	</requires>

	<plugin
		download-size="0"
		id="com.liferay.ide.workspace.core"
		install-size="0"
		unpack="false"
		version="${version}"
	/>

	<plugin
		download-size="0"
		id="com.liferay.ide.workspace.ui"
		install-size="0"
		unpack="false"
		version="${version}"
	/>

	<plugin
		download-size="0"
		id="com.liferay.ide.workspace.service.core"
		install-size="0"
		unpack="false"
		version="${version}"
	/>

	<plugin
		download-size="0"
		id="com.liferay.ide.workspace.service.ui"
		install-size="0"
		unpack="false"
		version="${version}"
	/>

</feature>